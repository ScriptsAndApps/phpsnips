echo  "<br>";
if(isset($_SERVER['HTTP_X_FORWARDED_FOR']))
echo  "REMOTE_ADDR: ".$_SERVER['HTTP_X_FORWARDED_FOR'];
echo  "<br>";
if(isset($_SERVER['HTTP_CLIENT_IP']))
echo  "REMOTE_ADDR: ".$_SERVER['HTTP_CLIENT_IP'];

phpinfo();

php?>
